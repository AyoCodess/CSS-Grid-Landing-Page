n$font-primary: \"Nunito\", sans-serif;\n$font-display: \"Josefin Sans\", sans-serif;\n\n*,\n*::before,\n*::after {\n    margin: 0;\n    padding: 0;\n    box-sizing: inherit;\n}\n\nhtml {\n    box-sizing: border-box;\n    font-size: 62.5%; //1 rem = 10px\n}\n\nbody {\n    font-family: $font-primary;\n    color: $color-grey-dark-2;\n    font-weight: 300;\n    line-height: 1.6;\n}\n\n.container {\n    display: grid;\n\n    grid-template-rows: 80vh min-content 40vw repeat(3, min-content);\n    grid-template-columns:\n        [sidebar-start] 8rem [sidebar-end full-start] minmax(6rem, 1fr)\n        [center-start] repeat(\n            8,\n            [col-start] minmax(min-content, 14rem) [col-end]\n        )\n        [center-end]\n        minmax(6rem, 1fr) [full-end];\n\n    & > * {\n    }\n}\n","sass/_typography.scss":"@mixin base-style {\n    font-family: $font-display;\n    font-weight: 400;\n}\n\n.heading-1 {\n}\n\n.heading-2 {\n    font-size: 4rem;\n    font-style: italic;\n    line-height: 1;\n\n    &--light {\n        color: $color-grey-light-1;\n    }\n\n    &--dark {\n        color: $color-grey-dark-1;\n    }\n}\n\n.heading-3 {\n    font-size: 1.6rem;\n    color: $color-primary;\n    text-transform: uppercase;\n}\n\n.heading-4 {\n    @include base-style;\n    font-size: 1.9rem;\n\n    &--light {\n        color: $color-grey-light-1;\n    }\n\n    &--dark {\n        color: $color-grey-dark-1;\n    }\n}\n\n.btn {\n    background-color: $color-primary;\n    color: white;\n    border: none;\n    border-radius: 0;\n    font-family: $font-display;\n    text-transform: uppercase;\n    padding: 1.8rem 3rem;\n    cursor: pointer;\n    transition: all 0.4s;\n    outline: none;\n\n    &:hover {\n        background-color: $color-primary-dark;\n    }\n}\n\n// helper classes of margin\n\n.mb-small {\n    margin-bottom: 2rem;\n}\n\n.mb-medium {\n    margin-bottom: 3rem;\n}\n\n.mb-large {\n    margin-bottom: 4rem;\n}\n\n.mb-huge {\n    margin-bottom: 8rem;\n}\n","sass/_sidebar.scss":".sidebar {\n    background-color: $color-primary;\n\n    grid-row: 1 / -1;\n    grid-column: sidebar-start / sidebar-end;\n}\n","sass/_header.scss":".header {\n    background-color: $color-grey-dark-1;\n\n    grid-row: 1 / 2;\n    grid-column: full-start / col-start 6;\n}\n","sass/_realtors.scss":".realtors {\n    background-color: $color-secondary;\n\n    grid-row: 1/ 2;\n\n    grid-column: col-start 6 / full-end;\n}\n","sass/_features.scss":".features {\n    grid-row: 2 / 3;\n    grid-column: center-start / center-end;\n\n    margin: 15rem 0;\n    display: grid;\n    // grid-template-columns: repeat(3, 1fr);\n\n    grid-template-columns: repeat(auto-fit, minmax(25rem, 1fr));\n\n    grid-gap: 6rem;\n\n    align-items: start;\n}\n\n.feature {\n    display: grid;\n\n    grid-template-columns: min-content 1fr;\n    grid-row-gap: 1.5rem;\n    grid-column-gap: 2.5rem;\n\n    &__icon {\n        fill: $color-primary;\n        width: 4.5rem;\n        height: 4.5rem;\n\n        grid-row: 1 / span 2;\n    }\n\n    &__text {\n        font-size: 1.7rem;\n    }\n}\n","sass/_story.scss":".story {\n    &__pictures {\n        background-color: $color-primary;\n        background-image: linear-gradient(\n                rgba($color-primary, 0.5),\n                rgba($color-primary, 0.5)\n            ),\n            url(../img/back.jpg);\n\n        grid-row: 3 / 4;\n        grid-column: full-start / col-end 4;\n\n        display: grid;\n        grid-template-rows: repeat(6, 1fr);\n        grid-template-columns: repeat(6, 1fr);\n        align-items: center;\n    }\n\n    &__img--1 {\n        width: 100%;\n\n        grid-column: 2 / 6;\n        grid-row: 2 / 6;\n        box-shadow: 0 2rem 5rem rgba(black, 0.1);\n    }\n\n    &__img--2 {\n        width: 115%;\n        grid-column: 4 / 7;\n        grid-row: 4 / 6;\n        z-index: 10;\n\n        box-shadow: 0 2rem 5rem rgba(black, 0.1);\n    }\n\n    &__content {\n        /// flex box solution\n        // display: flex;\n        // flex-direction: column;\n        // justify-content: center;\n        // align-items: flex-start;\n\n        /// grid solution\n\n        display: grid;\n        align-content: center;\n        justify-items: start;\n\n        background-color: $color-grey-light-1;\n\n        grid-row: 3 / 4;\n\n        grid-column: col-end 4 / full-end;\n\n        padding: 6rem 8vw;\n    }\n\n    &__text {\n        font-size: 1.5rem;\n        font-style: italic;\n        margin-bottom: 4rem;\n    }\n}\n","sass/_homes.scss":".homes {\n    grid-row: 4 / 5;\n    grid-column: center-start / center-end;\n    margin: 15rem 0;\n\n    display: grid;\n    grid-template-columns: repeat(auto-fit, minmax(25rem, 1fr));\n\n    grid-gap: 7rem;\n}\n\n.home {\n    background-color: $color-grey-light-1;\n\n    &__img {\n        width: 100%;\n    }\n\n    &__like {\n        fill: $color-primary;\n        height: 2rem;\n        width: 2rem;\n    }\n\n    &__name {\n    }\n\n    &__location,\n    &__rooms,\n    &__area,\n    &__price {\n        svg {\n            fill: $color-primary;\n            height: 2rem;\n            width: 2rem;\n        }\n\n        p {\n        }\n    }\n}\n","sass/_gallery.scss":".gallery {\n    background-color: $color-grey-dark-1;\n\n    grid-row: 5 / 6;\n    grid-column: full-start / full-end;\n}\n","sass/_footer.scss":".footer {\n    background-color: $color-secondary;\n\n    grid-row: 6 / 7;\n    grid-column: full-start / full-end;\n}\n"},"lineCount":null}},"error":null,"hash":"47c75073908795b10063e4ec146caf9f","cacheData":{"env":{}}}